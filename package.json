{
  "name": "compact-db",
  "version": "0.0.3",
  "description": "A lightweight embedded database inspired from leveldb but operate in sync mode",
  "keywords": [
    "embedded database",
    "snap-db",
    "level-db",
    "typescript",
    "object",
    "key-value"
  ],
  "author": "Beeno Tung <aabbcc1241@yahoo.com.hk> (https://beeno-tung.surge.sh)",
  "license": "BSD-2-Clause",
  "main": "dist/db.js",
  "types": "dist/db.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "postformat": "tslint -p . --fix",
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.build.json",
    "test": "npm run format && npm run build && jest",
    "prepublishOnly": "jest && npm run build"
  },
  "husky": {
    "hook": {
      "pre-commit": "npm run format"
    }
  },
  "directories": {
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@beenotung/tslib": "^16.35.0",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@types/node": "*",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-etc": "^1.13.7",
    "typescript": "^4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/compact-db.git"
  },
  "bugs": {
    "url": "https://github.com/beenotung/compact-db/issues"
  },
  "homepage": "https://github.com/beenotung/compact-db#readme"
}
